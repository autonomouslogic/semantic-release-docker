{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:allNonMajor",
    "group:recommended"
  ],
  "reviewers": [
    "kennethjor"
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchPackageNames": ["node"],
      "allowedVersions": "=16"
    },
    {
      "matchPackageNames": ["java"],
      "allowedVersions": "=11"
    }
  ]
}
